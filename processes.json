{
  "apps": [
    {
      "name": "WickrIO-Compliance-Bot",
      "args": [],
      "script": "node compliance_bot.js",
      "exec_interpreter": "none",
      "autorestart": true,
      "watch": [
        "package.json"
      ],
      "ignore_watch": [
        ".git"
      ],
      "env": {
        "tokens": {
        },
        "log_tokens": {
          "LOG_LEVEL": {
            "value": "info"
          },
          "LOG_FILE_SIZE": {
            "value": "10m"
          },
          "LOG_MAX_FILES": {
            "value": "5"
          }
        }
      },
      "out_file": "log.output",
      "error_file": "err.output"
    }
  ]
}
